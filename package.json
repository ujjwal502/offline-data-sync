{
  "name": "offline-data-sync",
  "version": "1.0.6",
  "description": "Offline-first data synchronization library with conflict resolution",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json && node scripts/fix-cjs-exports.js",
    "build:types": "tsc -p tsconfig.types.json",
    "prepublishOnly": "npm run build",
    "link": "cd dist && npm link"
  },
  "keywords": [
    "offline",
    "sync",
    "indexeddb",
    "typescript"
  ],
  "author": "ujjwaltiwari2@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ujjwal502/offline-data-sync"
  },
  "bugs": {
    "url": "https://github.com/ujjwal502/offline-data-sync/issues"
  },
  "homepage": "https://github.com/ujjwal502/offline-data-sync#readme",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "rimraf": "^5.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "idb": "^8.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
